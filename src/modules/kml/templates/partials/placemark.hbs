<Placemark>
  <name>{{process.number}}</name>
  <visibility>1</visibility>
  <styleUrl>#{{process.folderId}}</styleUrl>

  <description>
   <![CDATA[
    {{process.descriptionHTML}}
   ]]>
  </description>

  <MultiGeometry>
    <Polygon>
      <tessellate>0</tessellate>
      <extrude>1</extrude>
      <altitudeMode>clampToGround</altitudeMode>
      <outerBoundaryIs>
        <LinearRing>
          <coordinates>{{process.coordinates}}</coordinates>
        </LinearRing>
      </outerBoundaryIs>
    </Polygon>
  </MultiGeometry>

</Placemark>
